{
	"build": {
		"beforeDevCommand": "pnpm run dev",
		"beforeBuildCommand": "node -r @swc-node/register scripts/update-tauri-version-to-next.ts && pnpm run generate",
		"devPath": "http://localhost:3000",
		"distDir": "../.output/public",
		"withGlobalTauri": false
	},
	"package": {
		"productName": "wiwipaccer",
		"version": "0.0.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"shell": {
				"all": false,
				"open": true
			},
			"window": {
				"all": false,
				"startDragging": true
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "Copyright Â© 2023 Meadowsys\nLicense: AGPL-3.0-only",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "gay.kiwin.wiwipaccer",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null,
				"minimumSystemVersion": "10.14"
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": null
		},
		"systemTray": {
			"iconPath": "icons/systemtray.png",
			"iconAsTemplate": true,
			"menuOnLeftClick": true
		},
		"updater": {
			"active": false
		},
		"macOSPrivateApi": true
	}
}
